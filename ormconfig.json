{
    "type": "mysql",
    "url": "mysql://user:password@127.0.0.1:3306/db",
    "synchronize": false,
    "entities": [
      "dist/**/*.entity.js"
    ],
    "migrations": [
      "src/migrations/*{.ts,.js}"
    ],
    "migrationsTableName": "migrations",
    "ssl": false,
    "migrationsRun": true,
    "subscribers": [
      null
    ],
    "logging": [
      "error"
    ],
    "logger": "advanced-console",
    "cli": {
      "migrationsDir": "src/migrations"
    }
  }